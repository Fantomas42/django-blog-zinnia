{% load tagging_tags tz %}{% localtime off %}<?xml version="1.0" encoding="UTF-8"?>
<!-- This is a WordPress eXtended RSS file generated by Zinnia {{ version }} as an export of your blog. -->
<!-- It contains information about your blog's posts, comments, and categories. -->
<!-- You may use this file to transfer that content from one site to another. -->
<!-- This file is not intended to serve as a complete backup of your blog. -->

<!-- generator="Zinnia/{{ version }}" created="{% now "Y-m-d H:i" %}"-->
<rss version="2.0"
     xmlns:excerpt="http://wordpress.org/export/1.1/excerpt/"
     xmlns:content="http://purl.org/rss/1.0/modules/content/"
     xmlns:wfw="http://wellformedweb.org/CommentAPI/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:wp="http://wordpress.org/export/1.1/">
  <channel>
    <title>{{ site.name }}</title>
    <link>{{ site_url }}</link>
    <description>Blog exported from django-blog-zinnia</description>
    <pubDate>{% now "r" %}</pubDate>
    <generator>Zinnia {{ version }}</generator>
    <language>{{ language }}</language>
    <wp:wxr_version>1.0</wp:wxr_version>
    <wp:base_site_url>{{ site_url }}</wp:base_site_url>
    <wp:base_blog_url>{{ site_url }}{% url 'zinnia_entry_archive_index' %}</wp:base_blog_url>
    {% for category in categories %}
    <wp:category>
      <wp:category_nicename>{{ category.slug }}</wp:category_nicename>
      <wp:category_parent>{{ category.parent.title }}</wp:category_parent>
      <wp:cat_name><![CDATA[{{ category.title }}]]></wp:cat_name>
      <wp:category_description><![CDATA[{{ category.description }}]]></wp:category_description>
    </wp:category>
    {% endfor %}{% for tag in tags %}
    <wp:tag>
      <wp:tag_slug>{{ tag.name }}</wp:tag_slug>
      <wp:tag_name><![CDATA[{{ tag.name }}]]></wp:tag_name>
    </wp:tag>
    {% endfor %}{% for entry in entries %}
    <item>
      <title>{{ entry.title }}</title>
      <link>{{ site_url }}{{ entry.get_absolute_url }}</link>
      <pubDate>{{ entry.creation_date|date:"r" }}</pubDate>
      <dc:creator><![CDATA[{% with author=entry.authors.all.0 %}{% if author %}{{ author }}{% else %}Anonymous{% endif %}{% endwith %}]]></dc:creator>
      {% for category in entry.categories.all %}
      <category domain="category" nicename="{{ category.slug }}"><![CDATA[{{ category.title }}]]></category>
      {% endfor %}{% tags_for_object entry as tag_list %}{% for tag in tag_list %}
      <category domain="post_tag" nicename="{{ tag }}"><![CDATA[{{ tag }}]]></category>
      {% endfor %}
      <guid isPermaLink="true">{{ site_url }}{{ entry.get_absolute_url }}</guid>
      <description></description>
      <content:encoded><![CDATA[{{ entry.html_content|safe }}]]></content:encoded>
      <excerpt:encoded><![CDATA[{{ entry.excerpt|safe }}]]></excerpt:encoded>
      <wp:post_id>{{ entry.pk }}</wp:post_id>
      <wp:post_date>{{ entry.creation_date|date:"Y-m-d H:i:s" }}</wp:post_date>
      <wp:post_date_gmt>{{ entry.creation_date|date:"Y-m-d H:i:s" }}</wp:post_date_gmt>
      <wp:comment_status>{% if entry.comment_enabled %}open{% else %}closed{% endif %}</wp:comment_status>
      <wp:ping_status>{% if entry.pingback_enabled %}open{% else %}closed{% endif %}</wp:ping_status>
      <wp:track_status>{% if entry.trackback_enabled %}open{% else %}closed{% endif %}</wp:track_status>
      <wp:post_name>{{ entry.slug }}</wp:post_name>
      <wp:status>{% if entry.is_visible %}{% if entry.login_required %}private{% else %}publish{% endif %}{% else %}draft{% endif %}</wp:status>
      <wp:post_parent>0</wp:post_parent>
      <wp:menu_order>0</wp:menu_order>
      <wp:post_type>post</wp:post_type>
      <wp:post_password>{{ entry.password }}</wp:post_password>
      <wp:is_sticky>{{ entry.featured|yesno:"1,0" }}</wp:is_sticky>
      <wp:postmeta>
        <wp:meta_key>_last_update</wp:meta_key>
        <wp:meta_value><![CDATA[{{ entry.last_update|date:"c"}}]]></wp:meta_value>
      </wp:postmeta>{% if entry.start_publication %}
      <wp:postmeta>
        <wp:meta_key>_start_publication</wp:meta_key>
        <wp:meta_value><![CDATA[{{ entry.start_publication|date:"c"}}]]></wp:meta_value>
      </wp:postmeta>{% endif %}{% if entry.end_publication %}
      <wp:postmeta>
        <wp:meta_key>_end_publication</wp:meta_key>
        <wp:meta_value><![CDATA[{{ entry.end_publication|date:"c"}}]]></wp:meta_value>
      </wp:postmeta>{% endif %}
      {% for comment in entry.comments %}
      <wp:comment>
        <wp:comment_id>{{ comment.id }}</wp:comment_id>
        <wp:comment_author><![CDATA[{{ comment.name }}]]></wp:comment_author>
        <wp:comment_author_email>{{ comment.email }}</wp:comment_author_email>
        <wp:comment_author_url>{{ comment.url }}</wp:comment_author_url>
        <wp:comment_author_IP>{{ comment.ip_address }}</wp:comment_author_IP>
        <wp:comment_date>{{ comment.submit_date|date:"Y-m-d H:i:s" }}</wp:comment_date>
        <wp:comment_date_gmt>{{ comment.submit_date|date:"Y-m-d H:i:s" }}</wp:comment_date_gmt>
        <wp:comment_content><![CDATA[{{ comment.comment }}]]></wp:comment_content>
        <wp:comment_approved>1</wp:comment_approved>
        <wp:comment_type></wp:comment_type>
        <wp:comment_parent>0</wp:comment_parent>
        <wp:comment_user_id>0</wp:comment_user_id>
      </wp:comment>
      {% endfor %}{% for comment in entry.pingbacks %}
      <wp:comment>
        <wp:comment_id>{{ comment.id }}</wp:comment_id>
        <wp:comment_author><![CDATA[{{ comment.name }}]]></wp:comment_author>
        <wp:comment_author_email>{{ comment.email }}</wp:comment_author_email>
        <wp:comment_author_url>{{ comment.url }}</wp:comment_author_url>
        <wp:comment_author_IP>{{ comment.ip_address }}</wp:comment_author_IP>
        <wp:comment_date>{{ comment.submit_date|date:"Y-m-d H:i:s" }}</wp:comment_date>
        <wp:comment_date_gmt>{{ comment.submit_date|date:"Y-m-d H:i:s" }}</wp:comment_date_gmt>
        <wp:comment_content><![CDATA[{{ comment.comment }}]]></wp:comment_content>
        <wp:comment_approved>1</wp:comment_approved>
        <wp:comment_type>pingback</wp:comment_type>
        <wp:comment_parent>0</wp:comment_parent>
        <wp:comment_user_id>0</wp:comment_user_id>
      </wp:comment>
      {% endfor %}{% for comment in entry.trackbacks %}
      <wp:comment>
        <wp:comment_id>{{ comment.id }}</wp:comment_id>
        <wp:comment_author><![CDATA[{{ comment.name }}]]></wp:comment_author>
        <wp:comment_author_email>{{ comment.email }}</wp:comment_author_email>
        <wp:comment_author_url>{{ comment.url }}</wp:comment_author_url>
        <wp:comment_author_IP>{{ comment.ip_address }}</wp:comment_author_IP>
        <wp:comment_date>{{ comment.submit_date|date:"Y-m-d H:i:s" }}</wp:comment_date>
        <wp:comment_date_gmt>{{ comment.submit_date|date:"Y-m-d H:i:s" }}</wp:comment_date_gmt>
        <wp:comment_content><![CDATA[{{ comment.comment }}]]></wp:comment_content>
        <wp:comment_approved>1</wp:comment_approved>
        <wp:comment_type>trackback</wp:comment_type>
        <wp:comment_parent>0</wp:comment_parent>
        <wp:comment_user_id>0</wp:comment_user_id>
      </wp:comment>
      {% endfor %}
    </item>
    {% endfor %}
  </channel>
</rss>{% endlocaltime %}
